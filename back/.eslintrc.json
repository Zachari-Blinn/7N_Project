{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        //"linebreak-style": "error",
        "semi": "error",
        "linebreak-style":"off",
        "no-shadow":"off",
        "no-console":"off",
        "consistent-return":"off",
        "no-inner-declarations":"off"
    }
}
